# pyenv stuff
export PYENV_ROOT="/home/adam/.pyenv"

export PATH="$PYENV_ROOT/bin:/home/adam/.local/bin:/home/adam/bin:/usr/local/bin:/usr/bin:/bin:/snap/bin"
export ZSH="/home/adam/.oh-my-zsh"
ZSH_THEME="minimal"
# CASE_SENSITIVE="true"
ENABLE_CORRECTION="true"
plugins=(git asdf ag bundler colored-man-pages colorize docker-compose docker-machine docker dotenv gem mix npm rails rake redis-cli ruby tmux yarn)

eval "$(pyenv init --path)"
source $ZSH/oh-my-zsh.sh

# The next line updates PATH for the Google Cloud SDK.
if [ -f '/home/adam/opt/google-cloud-sdk/path.zsh.inc' ]; then . '/home/adam/opt/google-cloud-sdk/path.zsh.inc'; fi

# The next line enables shell command completion for gcloud.
if [ -f '/home/adam/opt/google-cloud-sdk/completion.zsh.inc' ]; then . '/home/adam/opt/google-cloud-sdk/completion.zsh.inc'; fi
