export PATH="$HOME/bin:$HOME/.local/bin:$HOME/go/bin:/usr/local/bin:/usr/local/go/bin:/usr/bin:/bin:/usr/sbin:/sbin:$PLAN9/bin:$WASMTIME_HOME/bin:$JAVA_HOME/bin"
export GTK_THEME=Adwaita:dark
export XCURSOR_THEME=Adwaita
