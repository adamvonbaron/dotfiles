<labwc_config>
  <theme>
    <name></name>
    <font name="Ubuntu Mono" size="12" />
  </theme>

  <keyboard>
    <default />
    <keybind key="W-Return">
      <action name="Execute" command="foot" />
    </keybind>
    <keybind key="W-F4">
      <action name="None" />
    </keybind>
  </keyboard>

  <mouse>
    <default />
    <context name="Root">
      <mousebind button="Right" action="Press">
        <action name="ShowMenu" menu="some-custom-menu" />
      </mousebind>
    </context>
  </mouse>
</labwc_config>
