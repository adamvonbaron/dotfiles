export PLAN9="/usr/local/plan9"
export GOPATH="/Users/adam/go"

export PATH="/Users/adam/bin:/Users/adam/.local/bin:/Users/adam/go/bin:/Users/adam/.yarn/bin:/Users/adam/.ghcup/bin:/Users/adam/.pyenv/bin:/opt/homebrew/opt/llvm/bin:/opt/homebrew/bin:/opt/homebrew/sbin:/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin:/usr/local/plan9/bin"
export ZSH="/Users/adam/.oh-my-zsh"
ZSH_THEME="minimal"
# CASE_SENSITIVE="true"
ENABLE_CORRECTION="true"

# pyenv
PYENV_ROOT="$HOME/.pyenv"
eval "$(pyenv init --path)"

plugins=(git ag aws bundler celery colored-man-pages colorize docker-compose docker-machine docker dotenv gem mix npm pip pyenv python rails rake redis-cli ruby tmux yarn)
source $ZSH/oh-my-zsh.sh

# opam configuration
[[ ! -r /Users/adam/.opam/opam-init/init.zsh ]] || source /Users/adam/.opam/opam-init/init.zsh  > /dev/null 2> /dev/null

# The next line updates PATH for the Google Cloud SDK.
if [ -f '/Users/adam/opt/google-cloud-sdk/path.zsh.inc' ]; then . '/Users/adam/opt/google-cloud-sdk/path.zsh.inc'; fi

# The next line enables shell command completion for gcloud.
if [ -f '/Users/adam/opt/google-cloud-sdk/completion.zsh.inc' ]; then . '/Users/adam/opt/google-cloud-sdk/completion.zsh.inc'; fi

# rbenv
eval "$(rbenv init - zsh)"
